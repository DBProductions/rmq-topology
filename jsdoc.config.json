{
    "source": {
        "include": ["src"],
        "exclude": [],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "opts": {
        "template": "node_modules/clean-jsdoc-theme",
        "encoding": "utf8",             
        "destination": "./public/doc/",
        "theme_opts": {
            "theme": "light",
            "title": "rmq-technology"
        }
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "default": {
            "includeDate": false,
            "useLongnameInNav": true,
            "outputSourceFiles": true
        }
    }
}