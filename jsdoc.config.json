{
    "source": {
        "include": ["src"],
        "exclude": [],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "opts": {
        "template": "templates/default",
        "encoding": "utf8",             
        "destination": "./public/doc/"
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "default": {
            "includeDate": false,
            "useLongnameInNav": true,
            "outputSourceFiles": true
        }
    }
}